{
  "name": "CrowdSauce",
  "devDependencies": {
    "grunt": "^0.4.5",
    "grunt-browserify": "^4.0.1",
    "grunt-react": "^0.12.3",
    "standard": "^3.0.0"
  },
  "scripts": {
    "debugmode": "grunt && npm run debug_fire_node",
    "debugmodewindows": "grunt && npm run debug_fire_node_windows",
    "start": "grunt && npm run fire_node",
    "fire_node": "node app.js",
    "debug_fire_node": "CRS_DEBUG=1 node app.js",
    "debug_fire_node_windows": "set CRS_DEBUG=1&&node app",
    "test": "npm run style || npm run mocha",
    "style": "standard",
    "mocha": "mocha test/*.js"
  },
  "standard": {
    "ignore": [
      "/node_modules/",
      "/public/js/libs/"
    ]
  },
  "dependencies": {
    "async": "^1.5.2",
    "body-parser": "^1.15.0",
    "express": "4.9.1",
    "fb": "1.0.2",
    "file-stream-rotator": "0.0.6",
    "jquery": "^2.2.0",
    "minimist": "*",
    "mocha": "^2.4.5",
    "morgan": "^1.7.0",
    "nodemailer": "2.3.0",
    "react": "^0.14.7",
    "react-bootstrap": "^0.28.3",
    "react-dnd": "^2.1.2",
    "react-dnd-html5-backend": "^2.1.2",
    "react-dom": "^0.14.7",
    "react-rating": "^0.2.0",
    "react-tag-input": "^2.2.3",
    "react-toggle": "2.0.1",
    "request": "^2.69.0",
    "require": "^2.4.20",
    "rethinkdb": "^2.2.1",
    "standard": "^6.0.7",
    "string-format": "^0.5.0",
    "swagger-node-express": "~2.0",
    "thinky": "^2.2.4"
  }
}
